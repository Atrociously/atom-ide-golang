{
    "name": "ide-golang",
    "main": "./lib/main",
    "version": "0.0.0",
    "description": "Go Language support in Atom using atom-ide-base and gopls",
    "keywords": [
        "ide",
        "golang",
        "autocomplete",
        "gopls"
    ],
    "engines": {
        "atom": ">=0.21.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Atrociously/ide-golang"
    },
    "bugs": {
        "url": "https://github.com/Atrociously/ide-golang/issues"
    },
    "license": "MIT",
    "dependencies": {
        "@types/atom": "^1.28.0",
        "@types/node": "^13.1.1",
        "atom-languageclient": "^0.9.9",
        "reflect-metadata": "^0.1.12",
        "tsyringe": "^4.0.1"
    },
    "devDependencies": {
        "atom-ts-transpiler": "^1.5.2",
        "typescript": "^3.2.2"
    },
    "atomTranspilers": [
        {
            "transpiler": "atom-ts-transpiler",
            "glob": "{!(node_modules)/**/,}*.ts?(x)",
            "options": {
                "compilerOptions": {
                    "target": "es6",
                    "module": "commonjs",
                    "resolveJsonModule": true
                },
                "cacheKeyFiles": [],
                "verbose": false
            }
        }
    ],
    "enhancedScopes": [
        "source.go",
        "go"
    ],
    "consumedServices": {
        "atom-ide-busy-signal": {
            "versions": {
                "0.1.0": "consumeBusySignal"
            }
        },
        "linter-indie": {
            "versions": {
                "2.0.0": "consumeLinterV2"
            }
        },
        "datatip": {
            "versions": {
                "0.1.0": "consumeDatatip"
            }
        },
        "signature-help": {
            "versions": {
                "0.1.0": "consumeSignatureHelp"
            }
        }
    },
    "providedServices": {
        "autocomplete.provider": {
            "versions": {
                "2.0.0": "provideAutocomplete"
            }
        },
        "code-format.onSave": {
            "versions": {
                "0.1.0": "provideOnSaveCodeFormat"
            }
        },
        "code-highlight": {
            "versions": {
                "0.1.0": "provideCodeHighlight"
            }
        },
        "definitions": {
            "versions": {
                "0.1.0": "provideDefinitions"
            }
        },
        "find-references": {
            "versions": {
                "0.1.0": "provideFindReferences"
            }
        },
        "outline-view": {
            "versions": {
                "0.1.0": "provideOutlines"
            }
        }
    }
}
